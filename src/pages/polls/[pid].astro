---
import Layout from "@layouts/Layout.astro"
import Header from "@partials/Header.vue"
import OpenPollSection from "@partials/OpenPollSection.vue"

const { pid } = Astro.params;
---

<Layout
	title="Faterium poll"
	description="Faterium Decentralized Application to work with Faterium Network."
	coverUrl="https://dapp.faterium.com/preview.png"
>
	<Header client:only />
	<OpenPollSection client:only pid={pid as string} />
</Layout>
